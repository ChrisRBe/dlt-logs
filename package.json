{
	"name": "dlt-logs",
	"displayName": "DLT-Logs",
	"description": "Open DLT diagnostic log and trace files with lifecycle detection, filters, time-sync, etc.",
	"version": "0.1.0",
	"license": "CC BY-NC-SA 4.0",
	"publisher": "mbehr1",
	"author": {
		"name": "Matthias Behr",
		"email": "mbehr+dltlogs@mcbehr.de"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mbehr1/dlt-logs.git"
	},
	"bugs": {
		"url": "https://github.com/mbehr1/dlt-logs/issues"
	},
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.dltOpenFile"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": [
			{
				"title": "DLT-Logs",
				"properties": {
					"dlt-logs.fileFilters": {
						"type": [
							"string"
						],
						"default": [
							"dlt",
							"DLT"
						],
						"description": "Specifies the file extensions that can be opened."
					}
				}
			}
		],
		"commands": [
			{
				"command": "extension.dltOpenFile",
				"title": "Open DLT file..."
			},
			{
				"command": "dlt-logs.adjustTime",
				"title": "adjust time..."
			}
		],
		"menus": {
			"editor/context": [
				{
					"group": "1_modification",
					"command": "dlt-logs.adjustTime",
					"when": "editorLangId == dlt-log"
				}
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "dlt-logs",
					"title": "DLT Explorer",
					"icon": "./media/dlt-logs.svg"
				}
			]
		},
		"views": {
			"dlt-logs": [
				{
					"id": "dltLifecycleExplorer",
					"name": "DLT Lifecycle Explorer"
				}
			]
		},
		"languages": [
			{
				"id": "dlt-log",
				"extensions": [
					".dlt",
					".DLT"
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.43.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.18.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"binary-parser": "^1.5.0",
		"vscode-extension-telemetry": "^0.1.2"
	}
}